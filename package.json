{
  "name": "nestjs-boilerplate",
  "version": "1.0.0",
  "description": "A production ready NestJS boilerplate with batteries included. No Kidding!",
  "author": "Vinayak Sarawagi <vinayaksarawagi25@gmail.com>",
  "private": false,
  "license": "MIT",
  "keywords": [
    "nestjs-boilerplate",
    "nestjs-objection",
    "nestjs-starter-template",
    "nestjs-template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/squareboat/nestjs-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/squareboat/nestjs-boilerplate/issues"
  },
  "homepage": "https://github.com/squareboat/nestjs-boilerplate",
  "scripts": {
    "start": "node dist/apps/rest/src/main.js",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:dev": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/nestjs-boilerplate/test/jest-e2e.json",
    "knex": "knex --knexfile knexfile.ts",
    "migrate:up": "npm run knex migrate:latest",
    "migrate:down": "npm run knex migrate:rollback",
    "migrate:make": "npm run knex migrate:make -- -x ts",
    "migrate:list": "npm run knex migrate:list",
    "seed:run": "npm run knex seed:run",
    "seed:make": "npm run knex seed:make -- -x ts"
  },
  "dependencies": {
    "@nestjs/axios": "^1.0.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.1.6",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/websockets": "^9.3.2",
    "@sentry/node": "^7.28.1",
    "@sentry/tracing": "^7.28.1",
    "@squareboat/nest-queue-strategy": "^0.0.9",
    "@squareboat/nestjs-objection": "^0.1.7",
    "@types/bcrypt": "^5.0.0",
    "aws-lambda": "^1.0.7",
    "aws-lambda-fastify": "^2.2.0",
    "aws-sdk": "^2.1197.0",
    "aws-serverless-express": "^3.4.0",
    "bcrypt": "^5.1.0",
    "bcrypt-nodejs": "^0.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "cli-table3": "^0.6.2",
    "ejs": "^3.1.8",
    "express-rate-limit": "^6.4.0",
    "fastify": "^4.5.2",
    "firebase-admin": "^11.2.0",
    "helmet": "^5.1.0",
    "html-minifier": "^4.0.0",
    "ioredis": "^5.2.2",
    "json2csv": "^5.0.7",
    "knex": "^2.2.0",
    "mjml": "^4.13.0",
    "moment": "^2.29.1",
    "nodemailer": "^6.7.8",
    "objection": "^3.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "query-string": "^7.0.1",
    "razorpay": "^2.8.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "serverless": "^3.22.0",
    "sharp": "^0.30.7",
    "ulid": "^2.3.0",
    "uuid": "^8.3.2",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.8",
    "@squareboat/nest-queue-redis": "^0.0.4",
    "@types/aws-lambda": "^8.10.102",
    "@types/aws-lambda-fastify": "^1.4.1",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.176",
    "@types/mjml": "^4.7.0",
    "@types/node": "^18.0.5",
    "@types/passport-jwt": "^3.0.8",
    "@types/pg": "^8.6.5",
    "@types/sharp": "^0.30.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@types/yargs-parser": "^21.0.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "file-loader": "^6.2.0",
    "husky": "^8.0.1",
    "i": "^0.3.7",
    "lint-staged": "^13.0.3",
    "npm": "^8.18.0",
    "prettier": "2.7.1",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^9.2.6",
    "serverless-plugin-typescript": "^2.1.2",
    "serverless-plugin-warmup": "^8.0.0",
    "serverless-webpack": "^5.9.0",
    "source-map": "^0.7.4",
    "supertest": "^6.1.6",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.4.4",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "@libs/boat/(.*)": "<rootDir>/libs/boat/src/$1",
      "@libs/boat": "<rootDir>/libs/boat/src",
      "^@libs/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@libs/mailman(|/.*)$": "<rootDir>/libs/mailman/src/$1",
      "^@libs/users(|/.*)$": "<rootDir>/libs/users/src/$1",
      "^@libs/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^libs/auth(|/.*)$": "<rootDir>/libs/auth/src/$1",
      "^libs/jobs(|/.*)$": "<rootDir>/libs/jobs/src/$1",
      "^libs/applications(|/.*)$": "<rootDir>/libs/applications/src/$1",
      "^@libs/tasks(|/.*)$": "<rootDir>/libs/tasks/src/$1",
      "^@libs/task-update-history(|/.*)$": "<rootDir>/libs/task-update-history/src/$1",
      "^@libs/task-eta-update-history(|/.*)$": "<rootDir>/libs/task-eta-update-history/src/$1",
      "^@libs/task-status-update-history(|/.*)$": "<rootDir>/libs/task-status-update-history/src/$1"
    }
  }
}